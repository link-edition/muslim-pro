      "rub_el_hizb_number": 240,
      "ruku_number": 550,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِۦلَـٰفِهِمْ رِحْلَةَ ٱلشِّتَآءِ وَٱلصَّيْفِ",
      "text_uthmani_tajweed": "إ<tajweed class=madda_normal>ِۦ</tajweed>لَ<tajweed class=madda_normal>ـٰ</tajweed>فِهِمْ رِحْلَةَ <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>شِّت<tajweed class=madda_obligatory>َا</tajweed>ٓءِ وَ<tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>صّ<tajweed class=madda_permissible>َي</tajweed>ْفِ <span class=end>٢</span>",
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 664138,
          "resource_id": 101,
          "text": "— уларга қиш ва ёз сафари осон қилиб қўйилгани учунгина;"
        }
      ]
    },
    {
      "id": 6196,
