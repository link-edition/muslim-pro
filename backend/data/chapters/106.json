      "rub_el_hizb_number": 240,
      "ruku_number": 550,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلْيَعْبُدُوا۟ رَبَّ هَـٰذَا ٱلْبَيْتِ",
      "text_uthmani_tajweed": "فَلْيَعْبُدُو<tajweed class=slnt>اْ</tajweed> رَبَّ هَ<tajweed class=madda_normal>ـٰ</tajweed>ذَا <tajweed class=ham_wasl>ٱ</tajweed>لْب<tajweed class=madda_permissible>َي</tajweed>ْتِ <span class=end>٣</span>",
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 664139,
          "resource_id": 101,
          "text": "Улар мана шу Уйнинг Парвардигорига ибодат қилсинлар!"
