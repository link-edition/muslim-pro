      "rub_el_hizb_number": 240,
      "ruku_number": 555,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
      "text_uthmani_tajweed": "وَ<tajweed class=ham_wasl>ٱ</tajweed>مْرَأَتُهُ<tajweed class=madda_normal>ۥ</tajweed> حَ<tajweed class=ghunnah>مّ</tajweed>َالَةَ <tajweed class=ham_wasl>ٱ</tajweed>لْحَطَ<tajweed class=qalaqah>ب</tajweed>ِ <span class=end>٤</span>",
      "page_number": 603,
      "juz_number": 30,
      "translations": [
        {
          "id": 664163,
          "resource_id": 101,
          "text": "Ва унинг ўтин орқалаган хотини,"
