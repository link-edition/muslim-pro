{
  "verses": [
    {
      "id": 5623,
      "verse_number": 1,
      "verse_key": "77:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلْمُرْسَلَـٰتِ عُرْفًا",
      "text_uthmani_tajweed": "وَ<tajweed class=ham_wasl>ٱ</tajweed>لْمُرْسَلَ<tajweed class=madda_normal>ـٰ</tajweed>تِ عُرْفًا <span class=end>١</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663566,
          "resource_id": 101,
          "text": "Пайдар-пай юбориладиган (шамол)ларга,"
        }
      ]
    },
    {
      "id": 5624,
      "verse_number": 2,
      "verse_key": "77:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْعَـٰصِفَـٰتِ عَصْفًا",
      "text_uthmani_tajweed": "فَ<tajweed class=ham_wasl>ٱ</tajweed>لْعَ<tajweed class=madda_normal>ـٰ</tajweed>صِفَ<tajweed class=madda_normal>ـٰ</tajweed>تِ عَصْفًا <span class=end>٢</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663567,
          "resource_id": 101,
          "text": "қутурадиган (бўрон)ларга қасам;"
        }
      ]
    },
    {
      "id": 5625,
      "verse_number": 3,
      "verse_key": "77:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلنَّـٰشِرَٰتِ نَشْرًا",
      "text_uthmani_tajweed": "وَ<tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed><tajweed class=ghunnah>نّ</tajweed>َ<tajweed class=madda_normal>ـٰ</tajweed>شِر<tajweed class=madda_normal>َٲ</tajweed>تِ نَشْرًا <span class=end>٣</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663568,
          "resource_id": 101,
          "text": "(Булутларни Аллоҳ хоҳлаган томонларга) ёйиб юборадиган,"
        }
      ]
    },
    {
      "id": 5626,
      "verse_number": 4,
      "verse_key": "77:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْفَـٰرِقَـٰتِ فَرْقًا",
      "text_uthmani_tajweed": "فَ<tajweed class=ham_wasl>ٱ</tajweed>لْفَ<tajweed class=madda_normal>ـٰ</tajweed>رِقَ<tajweed class=madda_normal>ـٰ</tajweed>تِ فَرْقًا <span class=end>٤</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663569,
          "resource_id": 101,
          "text": "(ҳақ билан ботилни ва ҳалол билан ҳаромни) ажратиб берадиган,"
        }
      ]
    },
    {
      "id": 5627,
      "verse_number": 5,
      "verse_key": "77:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُلْقِيَـٰتِ ذِكْرًا",
      "text_uthmani_tajweed": "فَ<tajweed class=ham_wasl>ٱ</tajweed>لْمُلْقِيَ<tajweed class=madda_normal>ـٰ</tajweed>تِ ذِكْرًا <span class=end>٥</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663570,
          "resource_id": 101,
          "text": "эслатма-ваҳий олиб тушадиган,"
        }
      ]
    },
    {
      "id": 5628,
      "verse_number": 6,
      "verse_key": "77:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عُذْرًا أَوْ نُذْرًا",
      "text_uthmani_tajweed": "عُذْرًا أَوْ نُذْرًا <span class=end>٦</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663571,
          "resource_id": 101,
          "text": "(мўмин бандаларни) узр-мағфират қилиш учун ё (кофирларни Охират азобидан) огоҳлантириш учун (ваҳий олиб тушадиган фаришта)ларга (яъни, юқорида мазкур бўлган барча нарсаларга) қасамки,"
        }
      ]
    },
    {
      "id": 5629,
      "verse_number": 7,
      "verse_key": "77:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ",
      "text_uthmani_tajweed": "إِ<tajweed class=ghunnah>نّ</tajweed>َمَا تُوعَدُونَ لَو<tajweed class=madda_normal>َٲ</tajweed>قِعٌ <span class=end>٧</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663572,
          "resource_id": 101,
          "text": "ҳеч шак-шубҳасиз, сизларга ваъда қилинаётган нарса (Қиёмат ва ҳисоб-китоб) воқеъ бўлгувчидир!"
        }
      ]
    },
    {
      "id": 5630,
      "verse_number": 8,
      "verse_key": "77:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
      "text_uthmani_tajweed": "فَإِذَا <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed><tajweed class=ghunnah>نّ</tajweed>ُجُومُ طُمِسَتْ <span class=end>٨</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663573,
          "resource_id": 101,
          "text": "Бас, қачон юлдузлар (нурлари сўниб) ўчирилгач;"
        }
      ]
    },
    {
      "id": 5631,
      "verse_number": 9,
      "verse_key": "77:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
      "text_uthmani_tajweed": "وَإِذَا <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>سَّم<tajweed class=madda_obligatory>َا</tajweed>ٓءُ فُرِجَتْ <span class=end>٩</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663574,
          "resource_id": 101,
          "text": "Ва қачон осмон (қобиғи) ёрилиб-очилгач;"
        }
      ]
    },
    {
      "id": 5632,
      "verse_number": 10,
      "verse_key": "77:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
      "text_uthmani_tajweed": "وَإِذَا <tajweed class=ham_wasl>ٱ</tajweed>لْجِبَالُ نُسِفَتْ <span class=end>١٠</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663575,
          "resource_id": 101,
          "text": "Ва қачон тоғлар (қумлар каби) сочилгач;"
        }
      ]
    },
    {
      "id": 5633,
      "verse_number": 11,
      "verse_key": "77:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
      "text_uthmani_tajweed": "وَإِذَا <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>رُّسُلُ أُقِّتَتْ <span class=end>١١</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663576,
          "resource_id": 101,
          "text": "Ва қачон пайғамбарларга (уларни ёлғончи қилган қавмлари билан ҳисоб-китоб қилишлари учун) белгиланган вақт келгач, (ана ўша вақтда ваъда қилинган Қиёмат қойим бўлур)!"
        }
      ]
    },
    {
      "id": 5634,
      "verse_number": 12,
      "verse_key": "77:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
      "text_uthmani_tajweed": "لِأَىِّ يَوْمٍ أُجِّلَتْ <span class=end>١٢</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663577,
          "resource_id": 101,
          "text": "(Пайғамбар ўз қавмлари билан ҳисоб-китоб қилишлари) қай Кунга белгиланган эди?"
        }
      ]
    },
    {
      "id": 5635,
      "verse_number": 13,
      "verse_key": "77:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِيَوْمِ ٱلْفَصْلِ",
      "text_uthmani_tajweed": "لِيَوْمِ <tajweed class=ham_wasl>ٱ</tajweed>لْفَصْلِ <span class=end>١٣</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663578,
          "resource_id": 101,
          "text": "(Ҳақ билан ботил) ажратиладиган Кунга (белгилангандир)!"
        }
      ]
    },
    {
      "id": 5636,
      "verse_number": 14,
      "verse_key": "77:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
      "text_uthmani_tajweed": "وَم<tajweed class=madda_obligatory>َآ</tajweed> أَ<tajweed class=qalaqah>دْ</tajweed>رَ<tajweed class=madda_normal>ٮٰ</tajweed>كَ مَا يَوْمُ <tajweed class=ham_wasl>ٱ</tajweed>لْفَصْلِ <span class=end>١٤</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663579,
          "resource_id": 101,
          "text": "(Эй инсон), сен (ҳақ билан ботил) ажратиладиган Кун нима эканлигини қаердан ҳам билурсан?!"
        }
      ]
    },
    {
      "id": 5637,
      "verse_number": 15,
      "verse_key": "77:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٥</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663580,
          "resource_id": 101,
          "text": "У Кунда (ўша Кунни) ёлғон дегувчиларга ҳалокат бўлгай!"
        }
      ]
    },
    {
      "id": 5638,
      "verse_number": 16,
      "verse_key": "77:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
      "text_uthmani_tajweed": "أَلَمْ نُهْلِكِ <tajweed class=ham_wasl>ٱ</tajweed>لْأَوَّل<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٦</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663581,
          "resource_id": 101,
          "text": "Ахир Биз аввалги (ўз пайғамбарларини ёлғончи қилган уммат)ларни ҳалок қилмадикми?!"
        }
      ]
    },
    {
      "id": 5639,
      "verse_number": 17,
      "verse_key": "77:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ",
      "text_uthmani_tajweed": "ثُ<tajweed class=ghunnah>مّ</tajweed>َ نُتْبِعُهُمُ <tajweed class=ham_wasl>ٱ</tajweed>لْأَخِر<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٧</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663582,
          "resource_id": 101,
          "text": "Сўнгра кейинги (яъни, Муҳаммад алайҳис-салоту вас-саломни ёлғончи қилгувчи Макка кофирлари ва бошқа)ларни ҳам уларга эргаштирурмиз!"
        }
      ]
    },
    {
      "id": 5640,
      "verse_number": 18,
      "verse_key": "77:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
      "text_uthmani_tajweed": "كَذ<tajweed class=madda_normal>َٲ</tajweed>لِكَ نَفْعَلُ بِ<tajweed class=ham_wasl>ٱ</tajweed>لْمُ<tajweed class=qalaqah>جْ</tajweed>رِم<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٨</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663583,
          "resource_id": 101,
          "text": "Биз барча жиноятчи-кофирларни мана шундай (ҳалок) қилурмиз!"
        }
      ]
    },
    {
      "id": 5641,
      "verse_number": 19,
      "verse_key": "77:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>١٩</span>",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 663584,
          "resource_id": 101,
          "text": "У Кунда ўша Кунни ёлғон дегувчиларга ҳалокат бўлгай!"
        }
      ]
    },
    {
      "id": 5642,
      "verse_number": 20,
      "verse_key": "77:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ",
      "text_uthmani_tajweed": "أَلَمْ نَخْلُ<tajweed class=idgham_mutaqaribayn>ق</tajweed>كّ<tajweed class=idgham_shafawi>ُم م</tajweed>ّ<tajweed class=idgham_ghunnah>ِن م</tajweed>ّ<tajweed class=madda_obligatory>َا</tajweed>ٓ<tajweed class=idgham_ghunnah>ءٍ م</tajweed>َّه<tajweed class=madda_permissible>ِي</tajweed>نٍ <span class=end>٢٠</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663585,
          "resource_id": 101,
          "text": "Ахир Биз сизларни бир ҳақир сувдан яратмадикми?!"
        }
      ]
    },
    {
      "id": 5643,
      "verse_number": 21,
      "verse_key": "77:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ",
      "text_uthmani_tajweed": "فَجَعَلْنَ<tajweed class=madda_normal>ـٰ</tajweed>هُ فِى قَرَا<tajweed class=idgham_ghunnah>رٍ م</tajweed>َّك<tajweed class=madda_permissible>ِي</tajweed>نٍ <span class=end>٢١</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663586,
          "resource_id": 101,
          "text": "Сўнг у (сувни-нутфа)ни мустаҳкам қароргоҳда (она қорнида барқарор) қилиб қўймадикми?!"
        }
      ]
    },
    {
      "id": 5644,
      "verse_number": 22,
      "verse_key": "77:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ قَدَرٍ مَّعْلُومٍ",
      "text_uthmani_tajweed": "إِلَىٰ قَدَ<tajweed class=idgham_ghunnah>رٍ م</tajweed>َّعْل<tajweed class=madda_permissible>ُو</tajweed>مٍ <span class=end>٢٢</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663587,
          "resource_id": 101,
          "text": "Маълум муддатгача (яъни, ой-куни етиб туғилгунича она қорнида барқарор қилиб қўймадикми?!)"
        }
      ]
    },
    {
      "id": 5645,
      "verse_number": 23,
      "verse_key": "77:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ",
      "text_uthmani_tajweed": "فَقَدَرْنَا فَنِعْمَ <tajweed class=ham_wasl>ٱ</tajweed>لْقَ<tajweed class=madda_normal>ـٰ</tajweed>دِر<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٢٣</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663588,
          "resource_id": 101,
          "text": "Бас, Биз (бир томчи сувни комил инсон қилиб яратишга) қодир бўлдик. Нақадар қудратлидирмиз! <sup foot_note=145347>1</sup>"
        }
      ]
    },
    {
      "id": 5646,
      "verse_number": 24,
      "verse_key": "77:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٢٤</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663589,
          "resource_id": 101,
          "text": "У Кунда ёлғон дегувчиларга ҳалокат бўлгай!"
        }
      ]
    },
    {
      "id": 5647,
      "verse_number": 25,
      "verse_key": "77:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
      "text_uthmani_tajweed": "أَلَمْ نَ<tajweed class=qalaqah>جْ</tajweed>عَلِ <tajweed class=ham_wasl>ٱ</tajweed>لْأَرْضَ كِفَاتًا <span class=end>٢٥</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663590,
          "resource_id": 101,
          "text": "Ахир Биз Ерни йиғгувчи қилиб қўймадикми?!"
        }
      ]
    },
    {
      "id": 5648,
      "verse_number": 26,
      "verse_key": "77:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَحْيَآءً وَأَمْوَٰتًا",
      "text_uthmani_tajweed": "أَحْي<tajweed class=madda_obligatory>َا</tajweed>ٓ<tajweed class=idgham_ghunnah>ءً و</tajweed>َأَمْو<tajweed class=madda_normal>َٲ</tajweed>تًا <span class=end>٢٦</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663591,
          "resource_id": 101,
          "text": "Тирикларни ҳам, ўликларни ҳам (йиғгувчи қилиб қўймадикми?!) <sup foot_note=145348>1</sup>"
        }
      ]
    },
    {
      "id": 5649,
      "verse_number": 27,
      "verse_key": "77:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا",
      "text_uthmani_tajweed": "وَجَعَلْنَا فِيهَا رَو<tajweed class=madda_normal>َٲ</tajweed>سِىَ شَ<tajweed class=madda_normal>ـٰ</tajweed>مِخَ<tajweed class=madda_normal>ـٰ</tajweed><tajweed class=idgham_ghunnah>تٍ و</tajweed>َأَسْقَيْنَ<tajweed class=madda_normal>ـٰ</tajweed>ك<tajweed class=idgham_shafawi>ُم م</tajweed>ّ<tajweed class=madda_obligatory>َا</tajweed>ٓ<tajweed class=ikhafa>ءً ف</tajweed>ُرَاتًا <span class=end>٢٧</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663592,
          "resource_id": 101,
          "text": "Яна Биз (ерда) баланд тоғларни (пайдо) қилдик ва сизларни (ўша тоғлардан оқиб тушадиган) чучук-тотли сув билан суғордик."
        }
      ]
    },
    {
      "id": 5650,
      "verse_number": 28,
      "verse_key": "77:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٢٨</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663593,
          "resource_id": 101,
          "text": "У Кунда ёлғон дегувчиларга ҳалокат бўлгай."
        }
      ]
    },
    {
      "id": 5651,
      "verse_number": 29,
      "verse_key": "77:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "text_uthmani_tajweed": "ٱ<tajweed class=ikhafa>نط</tajweed>َلِق<tajweed class=madda_obligatory>ُوٓ</tajweed><tajweed class=slnt>اْ</tajweed> إِلَىٰ مَا كُ<tajweed class=ikhafa>نت</tajweed><tajweed class=ikhafa_shafawi>ُم ب</tajweed>ِه<tajweed class=madda_normal>ِۦ</tajweed> تُكَذِّب<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٢٩</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663594,
          "resource_id": 101,
          "text": "(У Кунда кофирларга дейилур): «Сизлар (ҳаёти-дунёдалик пайтингизда) ёлғон дегувчи бўлган нарсага (ҳисоб-китоб ва дўзах азобига) боринглар!"
        }
      ]
    },
    {
      "id": 5652,
      "verse_number": 30,
      "verse_key": "77:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ",
      "text_uthmani_tajweed": "ٱ<tajweed class=ikhafa>نط</tajweed>َلِق<tajweed class=madda_obligatory>ُوٓ</tajweed><tajweed class=slnt>اْ</tajweed> إِلَىٰ ظِ<tajweed class=ikhafa>لٍّ ذ</tajweed>ِى ثَلَ<tajweed class=madda_normal>ـٰ</tajweed>ثِ شُعَ<tajweed class=qalaqah>ب</tajweed>ٍ <span class=end>٣٠</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663595,
          "resource_id": 101,
          "text": "(Жаҳаннамда ёнаётган дўзахилардан кўтарилаётган бадбуй тутундан иборат, осмонга ўрлаган сари бўлиниб кетган) уч бўлакли «соя»га боринглар!"
        }
      ]
    },
    {
      "id": 5653,
      "verse_number": 31,
      "verse_key": "77:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
      "text_uthmani_tajweed": "لَّا ظَلِي<tajweed class=idgham_ghunnah>لٍ و</tajweed>َلَا يُغْنِى مِنَ <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>لَّهَ<tajweed class=qalaqah>ب</tajweed>ِ <span class=end>٣١</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663596,
          "resource_id": 101,
          "text": "Ҳеч қандай салқинлик бермайдиган ва (дўзах) алангасини ҳам тўсмайдиган («соя»га боринглар!)»"
        }
      ]
    },
    {
      "id": 5654,
      "verse_number": 32,
      "verse_key": "77:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ",
      "text_uthmani_tajweed": "إِ<tajweed class=ghunnah>نّ</tajweed>َهَا تَرْمِى بِشَرَ<tajweed class=ikhafa>رٍ ك</tajweed>َ<tajweed class=ham_wasl>ٱ</tajweed>لْقَصْرِ <span class=end>٣٢</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663597,
          "resource_id": 101,
          "text": "Дарвоқеъ, у (жаҳаннам ҳар томонга ҳар бири) қасрдек учқунларни отиб турур!"
        }
      ]
    },
    {
      "id": 5655,
      "verse_number": 33,
      "verse_key": "77:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ",
      "text_uthmani_tajweed": "كَأَ<tajweed class=ghunnah>نّ</tajweed>َهُ<tajweed class=madda_normal>ۥ</tajweed> جِمَ<tajweed class=madda_normal>ـٰ</tajweed>لَ<tajweed class=ikhafa>تٌ ص</tajweed>ُفْرٌ <span class=end>٣٣</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663598,
          "resource_id": 101,
          "text": "Худди сариқ туяларга ўхшаган (учқунларни отиб турур!)"
        }
      ]
    },
    {
      "id": 5656,
      "verse_number": 34,
      "verse_key": "77:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٣٤</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663599,
          "resource_id": 101,
          "text": "У Кунда ёлғон дегувчиларга ҳалокат бўлғай!"
        }
      ]
    },
    {
      "id": 5657,
      "verse_number": 35,
      "verse_key": "77:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا يَوْمُ لَا يَنطِقُونَ",
      "text_uthmani_tajweed": "هَ<tajweed class=madda_normal>ـٰ</tajweed>ذَا يَوْمُ لَا يَ<tajweed class=ikhafa>نط</tajweed>ِق<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٣٥</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663600,
          "resource_id": 101,
          "text": "Бу (Кун) улар сўзлай олмайдиган"
        }
      ]
    },
    {
      "id": 5658,
      "verse_number": 36,
      "verse_key": "77:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
      "text_uthmani_tajweed": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِر<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٣٦</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663601,
          "resource_id": 101,
          "text": "Ва улар учун узр айтишларига ҳам изн берилмайдиган Кундир!"
        }
      ]
    },
    {
      "id": 5659,
      "verse_number": 37,
      "verse_key": "77:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٣٧</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663602,
          "resource_id": 101,
          "text": "У Кунда ёлғон дегувчиларга ҳалокат бўлгай!"
        }
      ]
    },
    {
      "id": 5660,
      "verse_number": 38,
      "verse_key": "77:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ",
      "text_uthmani_tajweed": "هَ<tajweed class=madda_normal>ـٰ</tajweed>ذَا يَوْمُ <tajweed class=ham_wasl>ٱ</tajweed>لْفَصْلِ‌ۖ جَمَعْنَ<tajweed class=madda_normal>ـٰ</tajweed>كُمْ وَ<tajweed class=ham_wasl>ٱ</tajweed>لْأَوَّل<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٣٨</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663603,
          "resource_id": 101,
          "text": "Бу (ҳақ билан ботил) ажратиладиган Кундир! (Эй бу Кунни ёлғон дегувчи кимсалар, мана) Биз сизларни ҳам, аввалги (ўз пайғамбарларини ёлғончи қилган)ларни ҳам жамладик."
        }
      ]
    },
    {
      "id": 5661,
      "verse_number": 39,
      "verse_key": "77:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
      "text_uthmani_tajweed": "فَإِ<tajweed class=ikhafa>ن ك</tajweed>َانَ لَكُمْ كَيْ<tajweed class=ikhafa>دٌ ف</tajweed>َكِيد<tajweed class=madda_permissible>ُو</tajweed>نِ <span class=end>٣٩</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663604,
          "resource_id": 101,
          "text": "Мана, агар сизлар учун (дўзахдан қутқарадиган) бирон найранг бўлса, Менга ўша найрангларингизни кўрсатинглар-чи?"
        }
      ]
    },
    {
      "id": 5662,
      "verse_number": 40,
      "verse_key": "77:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "text_uthmani_tajweed": "وَيْ<tajweed class=idgham_ghunnah>لٌ ي</tajweed>َوْمَئِ<tajweed class=idgham_wo_ghunnah>ذٍ ل</tajweed>ِّلْمُكَذِّب<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٤٠</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663605,
          "resource_id": 101,
          "text": "У Кунда ёлғон дегувчиларга ҳалокат бўлгай!"
        }
      ]
    },
    {
      "id": 5663,
      "verse_number": 41,
      "verse_key": "77:41",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ",
      "text_uthmani_tajweed": "إِ<tajweed class=ghunnah>نّ</tajweed>َ <tajweed class=ham_wasl>ٱ</tajweed>لْمُتَّقِينَ فِى ظِلَ<tajweed class=madda_normal>ـٰ</tajweed><tajweed class=idgham_ghunnah>لٍ و</tajweed>َعُي<tajweed class=madda_permissible>ُو</tajweed>نٍ <span class=end>٤١</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663606,
          "resource_id": 101,
          "text": "Тақводор зотлар эса (У Кунда) албатта соя-салқинлар ва чашмаларда,"
        }
      ]
    },
    {
      "id": 5664,
      "verse_number": 42,
      "verse_key": "77:42",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
      "text_uthmani_tajweed": "وَفَو<tajweed class=madda_normal>َٲ</tajweed>كِهَ مِ<tajweed class=ghunnah>مّ</tajweed>َا يَشْتَه<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٤٢</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663607,
          "resource_id": 101,
          "text": "Ва кўнгиллари тусаган мева-чевалар устидадирлар."
        }
      ]
    },
    {
      "id": 5665,
      "verse_number": 43,
      "verse_key": "77:43",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
      "text_uthmani_tajweed": "كُلُو<tajweed class=slnt>اْ</tajweed> وَ<tajweed class=ham_wasl>ٱ</tajweed>شْرَبُو<tajweed class=slnt>اْ</tajweed> هَنِ<tajweed class=madda_obligatory>يٓـ</tajweed>ـ<tajweed class=iqlab>ـَٔۢا ب</tajweed>ِمَا كُ<tajweed class=ikhafa>نت</tajweed>ُمْ تَعْمَل<tajweed class=madda_permissible>ُو</tajweed>نَ <span class=end>٤٣</span>",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 663608,
          "resource_id": 101,
          "text": "(Уларга дейилур): «Қилиб ўтган (яхши) амалларингиз сабабли (ушбу жаннат ноз-неъматларини) пок билиб еб-ичаверинглар»."
        }
      ]
    },
    {
      "id": 5666,
      "verse_number": 44,
