      "sajdah_number": null,
      "text_uthmani": "وَٱلتِّينِ وَٱلزَّيْتُونِ",
      "text_uthmani_tajweed": "وَ<tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>تِّينِ وَ<tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>زَّيْت<tajweed class=madda_permissible>ُو</tajweed>نِ <span class=end>١</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664042,
          "resource_id": 101,
          "text": "Анжирга қасам, зайтунга қасам;"
        }
      ]
    },
    {
      "id": 6100,
      "verse_number": 2,
      "verse_key": "95:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَطُورِ سِينِينَ",
      "text_uthmani_tajweed": "وَطُورِ سِين<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٢</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664043,
          "resource_id": 101,
          "text": "(Аллоҳ таоло Мусо пайғамбар билан бевосита сўзлашган) Сино тоғига қасам;"
        }
      ]
    },
    {
      "id": 6101,
      "verse_number": 3,
      "verse_key": "95:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهَـٰذَا ٱلْبَلَدِ ٱلْأَمِينِ",
      "text_uthmani_tajweed": "وَهَ<tajweed class=madda_normal>ـٰ</tajweed>ذَا <tajweed class=ham_wasl>ٱ</tajweed>لْبَلَدِ <tajweed class=ham_wasl>ٱ</tajweed>لْأَم<tajweed class=madda_permissible>ِي</tajweed>نِ <span class=end>٣</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664044,
          "resource_id": 101,
          "text": "Мана шу тинч-осойишта шаҳар (Маккаи Мукаррама)га қасамки;"
        }
      ]
    },
    {
      "id": 6102,
      "verse_number": 4,
      "verse_key": "95:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍ",
      "text_uthmani_tajweed": "لَقَ<tajweed class=qalaqah>دْ</tajweed> خَلَ<tajweed class=qalaqah>قْ</tajweed>نَا <tajweed class=ham_wasl>ٱ</tajweed>لْإِ<tajweed class=ikhafa>نس</tajweed>َ<tajweed class=madda_normal>ـٰ</tajweed>نَ ف<tajweed class=madda_obligatory>ِىٓ</tajweed> أَحْسَنِ تَ<tajweed class=qalaqah>قْ</tajweed>و<tajweed class=madda_permissible>ِي</tajweed>مٍ <span class=end>٤</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664045,
          "resource_id": 101,
          "text": "Дарҳақиқат, Биз инсонни энг гўзал шаклу шамойилда яратдик."
        }
      ]
    },
    {
      "id": 6103,
      "verse_number": 5,
      "verse_key": "95:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ رَدَدْنَـٰهُ أَسْفَلَ سَـٰفِلِينَ",
      "text_uthmani_tajweed": "ثُ<tajweed class=ghunnah>مّ</tajweed>َ رَدَ<tajweed class=qalaqah>دْ</tajweed>نَ<tajweed class=madda_normal>ـٰ</tajweed>هُ أَسْفَلَ سَ<tajweed class=madda_normal>ـٰ</tajweed>فِل<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٥</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664046,
          "resource_id": 101,
          "text": "Сўнгра (у Биз ато этган шундай гўзал сурат ва мукаммал хилқатнинг шукрини қилмасдан, куфру туғён йўлига кетгач), уни асфала-софилинга (яъни, энг тубан дўзахга) қайтардик."
        }
      ]
    },
    {
      "id": 6104,
      "verse_number": 6,
      "verse_key": "95:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
      "text_uthmani_tajweed": "إِلَّا <tajweed class=ham_wasl>ٱ</tajweed>لَّذِينَ ءَامَنُو<tajweed class=slnt>اْ</tajweed> وَعَمِلُو<tajweed class=slnt>اْ</tajweed> <tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>صَّ<tajweed class=madda_normal>ـٰ</tajweed>لِحَ<tajweed class=madda_normal>ـٰ</tajweed>تِ فَلَهُمْ أَ<tajweed class=qalaqah>جْ</tajweed>رٌ غَيْرُ مَمْن<tajweed class=madda_permissible>ُو</tajweed>نٍ <span class=end>٦</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664047,
          "resource_id": 101,
          "text": "Фақат (қай) бир зотлар иймон келтирсалар ва яхши амаллар қилсалар, ана ўшалар учун битмас-туганмас ажр-мукофот бордир."
        }
      ]
    },
    {
      "id": 6105,
      "verse_number": 7,
      "verse_key": "95:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ",
      "text_uthmani_tajweed": "فَمَا يُكَذِّبُكَ بَعْدُ بِ<tajweed class=ham_wasl>ٱ</tajweed><tajweed class=laam_shamsiyah>ل</tajweed>دّ<tajweed class=madda_permissible>ِي</tajweed>نِ <span class=end>٧</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664048,
          "resource_id": 101,
          "text": "Бас, (эй инсон), энди (яъни, Аллоҳ таолонинг барча нарсага қодир Зот эканлигини билганингдан сўнг) нима сени (Қиёмат Кунида бандаларнинг қилиб ўтган амалларига бериладиган) жазони ёлғон дейишга мажбур қилур?!"
        }
      ]
    },
    {
      "id": 6106,
      "verse_number": 8,
      "verse_key": "95:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَـٰكِمِينَ",
      "text_uthmani_tajweed": "أَلَيْسَ <tajweed class=ham_wasl>ٱ</tajweed>للَّهُ بِأَحْكَمِ <tajweed class=ham_wasl>ٱ</tajweed>لْحَ<tajweed class=madda_normal>ـٰ</tajweed>كِم<tajweed class=madda_permissible>ِي</tajweed>نَ <span class=end>٨</span>",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 664049,
          "resource_id": 101,
          "text": "Ахир Аллоҳ ҳукм қилгувчиларнинг энг адолатлиси эмасми?! (Дарҳақиқат, Аллоҳ таоло Қиёмат Кунида энг одил ҳукм қилгувчидир)."
        }
      ]
    }
  ],
