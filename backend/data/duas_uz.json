[
  {
    "id": 1,
    "category": "Tonggi",
    "title": "Uyqudan uyg'onganda o'qiladigan duo",
    "arabic": "الْحَمْدُ للهِ الَّذِي أَحْيَانَا بَعْدَ مَا أَمَاتَنَا وَإِلَيْهِ النُّشُورُ",
    "transcription": "Alhamdulillahillaziy ahyana ba'da ma amatana va ilayhin-nushur.",
    "translation": "Bizni o'ldirgandan keyin tiriltirgan Allohga hamd bo'lsin va qaytish Ungadir."
  },
  {
    "id": 2,
    "category": "Tonggi",
    "title": "Tong otganda o'qiladigan duo",
    "arabic": "اللَّهُمَّ بِكَ أَصْبَحْنَا، وَبِكَ أَمْسَيْنَا، وَبِكَ نَحْيَا، وَبِكَ نَمُوتُ وَإِلَيْكَ النُّشُورُ",
    "transcription": "Allohumma bika asbahna va bika amsayna va bika nahya va bika namutu va ilaykan-nushur.",
    "translation": "Ey Allohim, Sening noming bilan tong ottirdik, Sening noming bilan kechga kirdik, Sening noming bilan yashaymiz, Sening noming bilan vafot etamiz va qaytish Sengadir."
  },
  {
    "id": 3,
    "category": "Kechki",
    "title": "Kech kirganda o'qiladigan duo",
    "arabic": "أَمْسَيْنَا وَأَمْسَى الْمُلْكُ لِلَّهِ، وَالْحَمْدُ لِلَّهِ",
    "transcription": "Amsayna va amsal-mulku lillah, valhamdulillah.",
    "translation": "Kechga kirdik, mulk ham Allohga xos bo'lgan holda kechga kirdi. Barcha hamd Allohga xosdir."
  },
  {
    "id": 4,
    "category": "Kechki",
    "title": "Uxlashdan oldin o'qiladigan duo",
    "arabic": "بِاسْمِكَ اللَّهُمَّ أَمُوتُ وَأَحْيَا",
    "transcription": "Bismikallohumma amutu va ahya.",
    "translation": "Ey Allohim, Sening isming bilan o'laman va tirilaman."
  },
  {
    "id": 5,
    "category": "Kundalik",
    "title": "Xojatxonaga kirishda",
    "arabic": "بِسْمِ اللهِ، اللَّهُمَّ إِنِّي أَعُوذُ بِكَ مِنَ الْخُبْثِ وَالْخَبَائِثِ",
    "transcription": "Bismillahi, Allohumma inniy a'uzu bika minal xubsi val xoba-is.",
    "translation": "Bismillah. Ey Alloh, men Sendan erkak va ayol shaytonlarning yomonligidan panoh tilayman."
  },
  {
    "id": 6,
    "category": "Kundalik",
    "title": "Xojatxonadan chiqishda",
    "arabic": "غُفْرَانَكَ",
    "transcription": "G'ufronaka.",
    "translation": "Sendan mag'firat so'rayman."
  },
  {
    "id": 7,
    "category": "Namoz",
    "title": "Masjidga kirishda",
    "arabic": "اللَّهُمَّ افْتَحْ لِي أَبْوَابَ رَحْمَتِكَ",
    "transcription": "Allohummaftah liy abvoba rohmatik.",
    "translation": "Ey Allohim, menga rahmating eshiklarini ochgin."
  },
  {
    "id": 8,
    "category": "Namoz",
    "title": "Masjiddan chiqishda",
    "arabic": "اللَّهُمَّ إِنِّي أَسْأَلُكَ مِنْ فَضْلِكَ",
    "transcription": "Allohumma inniy as'aluka min fazlik.",
    "translation": "Ey Allohim, Sendan fazlingni so'rayman."
  },
  {
    "id": 9,
    "category": "Safar",
    "title": "Safarga chiqishda ulovga minganda",
    "arabic": "سُبْحَانَ الَّذِي سَخَّرَ لَنَا هَذَا وَمَا كُنَّا لَهُ مُقْرِنِينَ",
    "transcription": "Subhanallaziy soxxoro lana haza vama kunna lahu muqriniyn.",
    "translation": "Bizga buni bo'ysundirib qo'ygan Zot pokdir. Biz bunga qodir emas edik."
  },
  {
    "id": 10,
    "category": "Oila",
    "title": "Uyga kirishda",
    "arabic": "بِسْمِ اللهِ وَلَجْنَا، وَبِسْمِ اللهِ خَرَجْنَا، وَعَلَى رَبِّنَا تَوَكَّلْنَا",
    "transcription": "Bismillahi valajna va bismillahi xorojna va a'la robbina tavakkalna.",
    "translation": "Allohning ismi bilan kirdik, Allohning ismi bilan chiqdik va Robbimizga tavakkal qildik."
  },
  {
    "id": 11,
    "category": "Kundalik",
    "title": "Ovqatlanishdan oldin",
    "arabic": "بِسْمِ اللهِ",
    "transcription": "Bismillah.",
    "translation": "Allohning ismi bilan."
  },
  {
    "id": 12,
    "category": "Kundalik",
    "title": "Ovqatlanib bo'lgach",
    "arabic": "الْحَمْدُ لِلَّهِ الَّذِي أَطْعَمَنَا وَسَقَانَا وَجَعَلَنَا مُسْلِمِينَ",
    "transcription": "Alhamdulillahillaziy at'amana va saqona va ja'alana muslimiyn.",
    "translation": "Bizni yedirgan, ichirgan va musulmonlardan qilgan Allohga hamd bo'lsin."
  },
  {
    "id": 13,
    "category": "Kundalik",
    "title": "Aksirgan kishiga javob",
    "arabic": "يَرْحَمُكَ اللهُ",
    "transcription": "Yarhamukalloh.",
    "translation": "Alloh senga rahm qilsin."
  },
  {
    "id": 14,
    "category": "Kundalik",
    "title": "Biron ish boshlaganda",
    "arabic": "بِسْمِ اللهِ الرَّحْمَنِ الرَّحِيمِ",
    "transcription": "Bismillahir rohmanir rohiym.",
    "translation": "Mehribon va rahmli Alloh nomi bilan."
  },
  {
    "id": 15,
    "category": "Namoz",
    "title": "Azonni eshitganda",
    "arabic": "Mazzinga ijobat qilish",
    "transcription": "Azondagi so'zlarni qaytarish",
    "translation": "Azondagi so'zlarni qaytarish"
  },
  {
    "id": 16,
    "category": "Namoz",
    "title": "Namozdan keyin",
    "arabic": "أَسْتَغْفِرُ اللهَ (٣ مرات)",
    "transcription": "Astag'firulloh (3 marta).",
    "translation": "Allohdan mag'firat so'rayman."
  },
  {
    "id": 17,
    "category": "Bemorlik",
    "title": "Kasallarni yo'qlaganda",
    "arabic": "لَا بَأْسَ طَهُورٌ إِنْ شَاءَ اللهُ",
    "transcription": "La ba'sa tohurun inshaAlloh.",
    "translation": "Hechqisi yo'q, xohlasa poklovchidir."
  },
  {
    "id": 18,
    "category": "Qabr",
    "title": "Qabristonga kirganda",
    "arabic": "السَّلَامُ عَلَيْكُمْ أَهْلَ الدِّيَارِ مِنَ الْمُؤْمِنِينَ",
    "transcription": "Assalamu aleykum ahlad diyari minal mu'miniyn.",
    "translation": "Assalomu alaykum ey mo'minlar diyori ahli."
  },
  {
